{
  "manifest_version": 3,
  "name": "WP slug field extender",
  "version": "1.0",
  "description": "Increases the number of characters that can fit in the 'slug' preview window.",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://cms.bankrate.com/wp/wp-admin/*"
      ]
    }
  ]
}